pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

function swimmer:new(x, color, num)
  local s = {}
  s.anim = {}
  s.anim.idle = 0
  s.anim.left = 1
  s.anim.right = 2
  s.spr = s.anim.idle
  s.x = x
  s.y = 248
  s.speed = 6
  s.left = false
  s.right = false
  s.dir = 'n'
  s.color = color
  s.num = num
  s.flip = false
  s.time = 0
  self.__index = self
  return setmetatable(s, self)
end

function swimmer:update()
  if game.state == "play" and btnp(0, self.num) and not btnp(1, self.num) then
    self.left = true
    self.spr = self.anim.left
  end
  if game.state == "play" and btnp(1, self.num) and not btnp(0, self.num) then
    self.right = true
    self.spr = self.anim.right
  end
  if game.state == "play" and btnp(4, self.num) and self:at_checkpoint() then
    self.flip = not self.flip
    self.speed = -self.speed
  end
  if self:at_finish() then
    game.state = "over"
    game.winner = self.num
  end

  if self.left and self.right then
    self.left = false
    self.right = false
    if self:can_move() then
      self.y -= self.speed
      if self.flip then
        if (self.y - cam.y > 96 and cam.y <= 136) then
          cam:move(-self.speed)
        end
      else
        if (self.y - cam.y < 32 and cam.y >= 0) then
          cam:move(-self.speed)
        end
      end
    end
  end

  if not self:at_finish() then
    self.time = time() - game.start_time
  end

end

function swimmer:at_checkpoint()
  return self.y == 14
end

function swimmer:at_finish()
  return self.y == 248 and self.flip
end

function swimmer:can_move()
  return (not self:at_checkpoint() and not self:at_finish())
    or (self:at_checkpoint() and self.flip)
end

function cam:move(distance)
  if self.y + distance >= 0 then
    self.y += distance
  else
    self.y = 0
  end
end

function _init()

  swimmer = {}
  s1 = swimmer:new(20, 8, 0)
  s2 = swimmer:new(44, 10, 1)
  s3 = swimmer:new(68, 11, 2)
  s4 = swimmer:new(92, 14, 3)

  swimmers = {}
  add(swimmers, s1)
  add(swimmers, s2)
  add(swimmers, s3)
  add(swimmers, s4)

  cam = {}
  cam.x = 0
  cam.y = 144

  game.start_time = time()
  game.state = "menu"
end

function _update()
  for s in all(swimmers) do
    s:update()
  end
  if game.state == "over" and btnp(5) then
    load("fastlane.p8")
  end
end

function _draw()
  cls()
  rectfill(0,0,128,8,5)
  if game.state == "play" then
    print(time() - game.start_time - 3, 10, 1, 7)
  elseif game.state == "over" then
    print("player " .. game.winner + 1 .. " wins!", 10, 1, 7)
  end
  clip(0,8,128,112)
  camera(cam.x, cam.y)
  map(0, 0, 0, 8, 16, 32)

  for s in all(swimmers) do
    pal(8, s.color)
    spr(s.spr, s.x, s.y, 1, 1, false, s.flip)
    if s:at_finish() then
      rectfill(s.x - 7, s.y - 5, s.x + 15, s.y -1, 1)
      print(s.time - 3, s.x - 7, s.y - 5, 10)
    end
  end

  if game.state == "init" then
    if time() - game.start_time < 1 then
      spr(18, 50, 200)
    elseif time() - game.start_time < 2 then
      spr(17, 50, 200)
    elseif time() - game.start_time < 3 then
      spr(16, 50, 200)
    else
      game.state = "play"
    end
  end

  camera()
end

function anim_swimmer(s, flx, fly)
  sx = 0
  sy = 0
  sw = 8
  sh = 8
  x = s.x
  y = s.y
  sspr(sx, sy, sw, sh, 40, 40, sw, sh + 1, flx, fly)
end

-- object, starting frame, number of frames, animation speed, flip x, flip y
function anim(o, sf, nf, sp, flx, fly)
  if(not o.a_ct) o.a_ct = 0
  if(not o.a_st) o.a_st = 0

  o.a_ct+=1

  if(o.a_ct % (30 / sp) == 0) then
    o.a_st+=1
    if(o.a_st == nf) o.a_st = 0
  end

  o.a_fr = sf + o.a_st
  spr(o.a_fr, o.x, o.y, 1, 1, flx, fly)
end

